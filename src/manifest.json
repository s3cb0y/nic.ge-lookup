{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Nic.ge Lookup",
  "description": "Fetches GE domain WHOIS information from https://nic.ge/",
  "author": "s3cb0y",
  "homepage_url": "https://github.com/s3cb0y/nic.ge-lookup",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon-16-dark.png",
    "32": "icons/icon-32-dark.png",
    "48": "icons/icon-48-dark.png",
    "96": "icons/icon-96-dark.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16-light.png",
      "32": "icons/icon-32-light.png",
      "48": "icons/icon-48-light.png",
      "96": "icons/icon-96-light.png"
    },
    "theme_icons": [
      {
        "light": "icons/icon-16-light.png",
        "dark": "icons/icon-16-dark.png",
        "size": 16
      },
      {
        "light": "icons/icon-32-light.png",
        "dark": "icons/icon-32-dark.png",
        "size": 32
      },
      {
        "light": "icons/icon-48-light.png",
        "dark": "icons/icon-48-dark.png",
        "size": 48
      },
      {
        "light": "icons/icon-96-light.png",
        "dark": "icons/icon-96-dark.png",
        "size": 96
      }
    ],
    "default_title": "Nic.ge Lookup",
    "default_popup": "popup/popup.html"
  }
}
